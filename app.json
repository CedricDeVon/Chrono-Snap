{
  "expo": {
    "name": "NimBit",
    "slug": "nimbit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logos-main/png/snack.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "NimBit"
    },
    "splash": {
      "image": "./assets/images/logos-main/png/snack.png",
      "imageWidth": 50,
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logos-main/png/snack.png",
        "backgroundColor": "#000000"
      },
      "package": "com.c3dric_d3v0n.NimBit"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logos-main/png/snack.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Ubuntu/Ubuntu-Regular.ttf",
            "./assets/fonts/Ubuntu/Ubuntu-Medium.ttf",
            "./assets/fonts/Ubuntu/Ubuntu-Light.ttf",
            "./assets/fonts/Ubuntu/Ubuntu-Bold.ttf"
          ]
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logos-main/png/snack.png",
          "imageWidth": 50,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "21a6e0e5-11ad-4e8c-a25f-3dfd5d12af40"
      }
    }
  }
}
